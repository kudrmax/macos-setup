{
  "title": "Launch MacOS apps",
  "rules": [
    {
      "description": "Launch Google Calendar on f4",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f4"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Google Calendar.app'"
            }
          ]
        }
      ]
    }
  ]
}